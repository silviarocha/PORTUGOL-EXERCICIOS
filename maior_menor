/*Ler dois valores (considere que não serão lidos valores iguais) 
e escrever o maior deles. */
programa {
	funcao inicio() {
	    inteiro valor1
	    inteiro valor2
	    
	    escreva("entre com um primeiro valor: ")
	    leia(valor1)
	    
	    escreva("entre com um segundo valor: ")
	    leia(valor2)
	    
	    se(valor1 > valor2){
	        escreva("o valor maior eh o: ", valor1)
	    } senao se(valor2 > valor1){
	        escreva("o valor maior eh o: ", valor2)
	    } senao {
	        escreva("invalido! valores iguais")
	    }
    }
}
